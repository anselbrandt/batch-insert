    "import sqlite3\n",
    "\n",
    "searchterm = \"donuts\"\n",
    "conn = sqlite3.connect(\"transcripts.db\")\n",
    "c = conn.cursor()\n",
    "\n",
    "c.execute(\"\"\"SELECT * FROM lines WHERE speech LIKE '%' || ? || '%'\"\"\", (searchterm,))\n",
    "\n",
    "results = c.fetchall()\n",
    "\n",
    "conn.commit()\n",
    "conn.close()\n",
    "\n",
    "\n",
    "len(results)"
   ]
